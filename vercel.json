{
  "version": 2,
  "installCommand": "git clone https://github.com/flutter/flutter.git --depth 1 -b stable && export PATH=$PWD/flutter/bin:$PATH && flutter --version && flutter config --enable-web && flutter pub get",
  "buildCommand": "export PATH=$PWD/flutter/bin:$PATH && flutter build web --release --base-href \"/\"",
  "outputDirectory": "build/web",
  "headers": [
    { "source": "/flutter_bootstrap.js", "headers": [ { "key": "Cache-Control", "value": "no-store" } ] },
    { "source": "/manifest.json", "headers": [ { "key": "Cache-Control", "value": "no-store" } ] }
  ],
  "routes": [
    { "src": "/flutter_bootstrap.js", "dest": "/flutter_bootstrap.js" },
    { "src": "/flutter.js", "dest": "/flutter.js" },
    { "src": "/main.dart.js", "dest": "/main.dart.js" },
    { "src": "/manifest.json", "dest": "/manifest.json" },
    { "src": "/favicon.png", "dest": "/favicon.png" },
    { "src": "/canvaskit/(.*)", "dest": "/canvaskit/$1" },
    { "handle": "filesystem" },
    { "src": "/(.*\\.(?:css|js|png|jpe?g|gif|svg|webp|ico|json|wasm|map|txt|ttf|woff2?))", "dest": "/$1" },
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/icons/(.*)", "dest": "/icons/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
