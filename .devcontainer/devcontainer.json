{
  "name": "Syncra Flutter Web Builder",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "dart-code.flutter",
        "dart-code.dart-code"
      ]
    }
  },
  "forwardPorts": [5000, 3000],
  "postCreateCommand": "chmod +x compile_and_deploy.sh && ./compile_and_deploy.sh",
  "remoteUser": "root"
}